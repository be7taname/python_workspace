class Calculate(object):
    def compEqu(self, equStack, equSymbol):
        if not equStack:
            equStack.append(equSymbol)
        
    def solution(self, s):
        """
        :type s: str
        :rtype: int
        """
        strLen = len(s)
        cnt = 0
        while cnt < strLen:
            cc = s[cnt]
            if cc >= '0' and cc <= '9':
                curNumStr = cc
                cnt = cnt + 1
                while cnt < strLen: # either (cc is not a number) or (cnt == strLen and cc is a number) 
                    cc = s[cnt]
                    if cc >= '0' and cc <= '9':
                        curNumStr += cc
                        cnt += 1
                    else:
                        break
                