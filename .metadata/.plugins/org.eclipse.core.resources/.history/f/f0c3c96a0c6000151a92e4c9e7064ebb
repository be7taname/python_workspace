class NumberToWords(object):
    def solution(self, num):
        """
        :type num: int
        :rtype: str
        """
        if num == 0: return "Zero"
        digits = ["One", "Two", "Three", "Four", "Five", 
                  "Six", "Seven", "Eight", "Nine", "Ten"]