class Permute(object):
    def solution(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        numsLen = len(nums)
        if numsLen == 1: return [[nums[0]]]
        res = []
        for i in range(numsLen):
            thisRes = self.solution(nums[0:i] + nums[i+1:])
            
                