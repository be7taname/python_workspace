class SimplifyPath(object):
    def solution(self, path):
        """
        :type path: str
        :rtype: str
        """
        pathLen = len(path)
        cnt = 0
        pathQ = []
        while cnt < pathLen:
            cc = path[cnt]
            if cc == '/':
                if not pathQ or pathQ[-1] != '/':
                    pathQ.append(cc)
            elif cc == '.':