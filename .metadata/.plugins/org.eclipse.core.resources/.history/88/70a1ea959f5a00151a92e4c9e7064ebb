class SortedListToBST(object):
    def genBstFromList(self, listHead, listLen):
        
        
    def solution(self, head):
        """
        :type head: ListNode
        :rtype: TreeNode
        """
        thisListNode = head
        listNodeCnt = 0
        while thisListNode != None:
            listNodeCnt += 1
            thisListNode = thisListNode.next
        leftTree = genBstFromList(head, listNodeCnt/2)