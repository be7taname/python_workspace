from common.ListNode import ListNode
class SortList(object):
    def solution(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        listLen = 1
        curp = head
        while curp.next:
            listLen += 1
            curp = curp.next
        if listLen == 1: return head
        else:
            dummy = ListNode()
            dummy.next = head
            halfWindow = 1
            while halfWindow < listLen:
                for i in range(listLen / (halfWindow * 2))
                hp = dummy