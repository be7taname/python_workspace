class CanJump(object):
    def solution(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        numZeros = nums.count(0)
        if numZeros == 0: return True
        while numZeros > 0:
            zeroIdx = nums.index(0)        