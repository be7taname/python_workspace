class FindRepeatedDnaSequences(object):
    def solution(self, s):
        """
        :type s: str
        :rtype: List[str]
        """
        allseq = dict()
        dnaMap = {'A':0, 'C':1, 'G':2 ,'T':3}
        curseq = 0
        for i in range(len(s)):
            curseq <<= 2
            curseq ^= dnaMap[s[i]]
            curseq &= 0xFFFFF
            if i < 9: continue