class LargestNumber(object):
    def mycmp(self, x, y):
        return x + y > y + x
    
    def solution(self, nums):
        """
        :type nums: List[int]
        :rtype: str
        """
        numstr = []
        maxLen = 0
        for num in nums:
            s = str(num)
            if maxLen < len(s): maxLen = len(s)
            numstr.append(s)
        numstrInSort = sorted(numstr, key = lambda x: x + 'A'*(maxLen-len(x)), reverse = True)
        return "".join(numstrInSort)
    