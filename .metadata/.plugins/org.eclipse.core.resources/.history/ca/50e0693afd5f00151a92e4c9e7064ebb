class Divide(object):
    def solution(self, dividend, divisor):
        """
        :type dividend: int
        :type divisor: int
        :rtype: int
        """
        pos = 1
        if dividend < 0:
            dividend = -dividend
        if divisor < 0:
            divisor = -divisor