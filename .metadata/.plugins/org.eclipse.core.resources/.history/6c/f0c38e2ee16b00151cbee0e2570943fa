class ContainsNearbyAlmostDuplicate(object):
    def solution(self, nums, k, t):
        """
        :type nums: List[int]
        :type k: int
        :type t: int
        :rtype: bool
        """
        for i in range(k+1):
            for j in range(i+1, k+1):
                if abs(nums[i] - nums[j]) <= t: return True
        
        