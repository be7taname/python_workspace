class Divide(object):
    def solution(self, dividend, divisor):
        """
        :type dividend: int
        :type divisor: int
        :rtype: int
        """
        pos = 1
        if dividend < 0:
            dividend = -dividend
            pos ^= 1
        if divisor < 0:
            divisor = -divisor
            pos ^= 1
        res = 0
        while dividend > divisor:
            curDiv = divisor << 1
            numShift = 1
            while dividend > curDiv:
                numShift += 1
                curDiv <<= 1
            