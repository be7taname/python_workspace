class EvalRPN(object):
    def solution(self, tokens):
        """
        :type tokens: List[str]
        :rtype: int
        """
        op = set(['+', '-', '*', '/'])
        numStack = []
        for cc in tokens:
            if cc in op:
                val2 = numStack.pop()
                val1 = numStack.pop()
                if cc == '+':
                    numStack.append(val1 + val2)
                elif cc == '-':
                    numStack.append(val1 - val2)
                elif cc == '*':
                    numStack.append(val1 * val2)
                else:
                    numStack.append(val1 / val2)
            else:
                numStack.append(int(cc))
                