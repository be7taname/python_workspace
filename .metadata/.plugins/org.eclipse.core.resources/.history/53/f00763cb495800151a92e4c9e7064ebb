class MaxSubArray(object):
    def solutions(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        numsLen = len(nums)
        if numsLen == 1: return nums[0]
        else:
            firstPos = 0
            max = nums[0]
            if max <= 0:
                findPos = 0
                for i in range(1, numsLen):
                    if nums[i] <= 0:
                        if nums[i] > max:
                            max = nums[i]
                    else:
                        findPos = 1
                        firstPos = i
                        break
                if findPos == 0: return max
            startPos = firstPos
            lastPos = firstPos
            cumsum = nums[firstPos]
            for i in range(firstPos+1, numsLen):
                cumsum += nums[i]
                if nums[i] >= 0:
                    lastPos = i
                else:
                    
                    
    