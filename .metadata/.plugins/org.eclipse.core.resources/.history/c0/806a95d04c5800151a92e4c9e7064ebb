class MaxSubArray(object):
    def solutions(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        numsLen = len(nums)
        if numsLen == 1: return nums[0]
        pmax = self.solutions(nums[1:numsLen])
        if nums[0] < 0:
            return (nums[0] if nums[0] > pmax else pmax)
        else:
            for i in range(len(nums)):    