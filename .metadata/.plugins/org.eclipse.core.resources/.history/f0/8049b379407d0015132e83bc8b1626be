class CanJump(object):
    def solution(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        maxIdx = 0
        numsLen = len(nums)
        for i in range(numsLen-1):
            if i > maxIdx: return False
            x = nums[i]
            if x + i > maxIdx:
                maxIdx = x + i