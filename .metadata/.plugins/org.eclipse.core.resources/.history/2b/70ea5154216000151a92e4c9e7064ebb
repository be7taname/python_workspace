class NumDecodings(object):
    def solution(self, s):
        """
        :type s: str
        :rtype: int
        """
        if len(s) == 0: return 0
        elif len(s) == 1: return 1
        else:
            is s[-1] == '0': way1 = 0
            else: way1 = self.solution(s[:-1])        