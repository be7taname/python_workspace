class MaxProduct(object):
    def solution(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        numsLen = len(nums)
        if numsLen == 1: return nums[0]
        elif numsLen == 2:
            if nums[0] <= 0 and nums[1] >= 0:
                return nums[1]
            elif nums[0] >= 0 and nums[1] <= 0:
                return nums[0]
            else:
                return nums[0]*nums[1]
        else:
