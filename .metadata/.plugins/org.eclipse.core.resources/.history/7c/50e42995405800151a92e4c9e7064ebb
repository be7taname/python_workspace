class MaxSubArray(object):
    def solutions(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        cumsum = []
        a = 0
        for i in range(len(nums)):
            a += nums[i]
            cumsum.append(a)
        minidx = min(range(len(nums)), lambda k: cumsum[k])
        maxidx = max(range(len(nums)), lambda k: cumsum[k])
        