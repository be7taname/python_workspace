class IsValidBST(object):
    def solutions(self, root):
        """
        :type root: TreeNode
        :rtype: bool
        """
        
    def MaxOrInvalid(self, root):
        if root.left == None and root.right == None:
            return root.val
        if root.left != None:
            leftMax = self.MaxOrInvalid(root.left)
            if leftMax == -1: return -1
            if leftMax > root.val: return -1
        