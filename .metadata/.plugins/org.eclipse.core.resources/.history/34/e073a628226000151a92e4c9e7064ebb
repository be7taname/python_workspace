class NumDecodings(object):
    def solution(self, s):
        """
        :type s: str
        :rtype: int
        """
        if len(s) == 0: return 0
        elif len(s) == 1: return 1
        else:
            if s[-1] == '0': way1 = 0
            else: way1 = self.solution(s[:-1])
            if s[-2] == '1' or (s[-2] == '2' and (s[-1] >= 0 and s[-1] <= 6)):
                way2 = self.solution(s[:-2])
            else:
                way2 = 0                