class MaxSubArray(object):
    def solutions(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        cumsum = []
        a = 0
        for i in range(len(nums)):
            a += nums[i]
            cumsum.append(a)
        minidx = cumsum.index(min(cumsum))
        maxidx = cumsum.index(max(cumsum))
        if minidx == maxidx: return nums[minidx]
        return sum(nums[minidx+1:maxidx+1])
    