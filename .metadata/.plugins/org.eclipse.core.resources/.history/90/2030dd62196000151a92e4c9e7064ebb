class ReverseWords(object):
    def solution(self, s):
        """
        :type s: str
        :rtype: str
        """
        sLen = len(s)
        wordList = []
        i = 0
        while i < sLen:
            cc = s[i]
            if cc != " ":
                curWord = cc
                i += 1
                while i < sLen:
                    cc = s[i]
                    if cc == " ": break
                    curWord += cc
                    i += 1
                wordList.append(curWord)
        revS = ""
        if wordList:
            