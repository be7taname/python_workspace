class CanFinish(object):
    def solutions(self, numCourses, prerequisites):
        """
        :type numCourses: int
        :type prerequisites: List[List[int]]
        :rtype: bool
        """
        if numCourses == 1: return True
        prereqLen = len(prerequisites)
        srtedPrereq = [-1] * prereqLen
