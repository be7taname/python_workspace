from common.ListNode import ListNode

class RotateRight(object):
    def solution(self, head, k):
        """
        :type head: ListNode
        :type k: int
        :rtype: ListNode
        """
        if k == 0: return head
        else:
            listLen = 1
            curp = head
            while curp.next:
                listLen += 1
                curp = curp.next
            tail = curp